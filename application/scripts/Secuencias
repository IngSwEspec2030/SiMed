-- Secuencias

UPDATE "simedSch"."EPS" SET "ID_EPS" = NEXTVAL(
'"simedSch"."EPS_SEQ"');

ALTER TABLE "simedSch"."EPS" ALTER COLUMN "ID_EPS" 
SET DEFAULT nextval('"simedSch"."EPS_SEQ"'::regclass);

